{
  "display-name": "Go Lang",
  "package-name": "go",
  "latest-version": "1.16",
  "1.16": {
    "url": "https://golang.org/dl/go1.16.windows-amd64.msi",
    "file-type": ".msi",
    "install-switches": ["/qn", "/quiet", "/norestart"],
    "uninstall-switches": ["/qn", "/quiet", "/norestart"],
    "custom-location": "INSTALLDIR=",
    "default-install-dir": "C:\\Program Files\\Go",
    "bin": [
       "<install-directory>\\bin\\go.exe", 
       "<install-directory>\\bin\\gofmt.exe", 
     ],
    "dependencies": []
  }
}
