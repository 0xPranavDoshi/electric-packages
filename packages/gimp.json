    "display-name": "GIMP",
    "package-name": "gimp",
    "latest-version": "2.10.22",
    "2.10.22": {
        "url": "https://download.gimp.org/mirror/pub/gimp/v2.10/windows/gimp-2.10.22-setup.exe",
        "url-type": ".exe",
